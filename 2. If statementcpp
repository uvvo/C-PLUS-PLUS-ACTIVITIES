//Menor, Harold O. BSCS1D
//Multiple if statements.
#include <stdio.h>
int main()
{
    int a, b;
    printf("enter the value of a:");
    scanf("%d", &a);
    printf("enter the value of b:");
    scanf("%d", &b);
    if (a>b)
    {
	printf("a is greater than b\n");
    }
    if (a<b)
    {
	printf("a is less than b\n");
    }
    if (a==b)
    {
	printf("a is equal to b\n");
    }
    printf("End ");
   
    return 0;
}
